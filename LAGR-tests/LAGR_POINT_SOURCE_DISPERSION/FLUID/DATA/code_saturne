#!/bin/bash

# Ensure the correct command is found:
#export PATH="/home/yohann-ws2/Code_Saturne/6.2-alpha/code_saturne-6.2-alpha/arch/Linux_x86_64/bin":$PATH

# Insert default command
cs_cmd=""
if test $# = 1; then
  if test -f $1; then
    cs_cmd=gui
  fi
elif test $# = 0; then
  cs_cmd=gui
fi

# Run command
\code_saturne $cs_cmd $@
