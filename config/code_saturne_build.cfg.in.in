datadir: @cfg_datadir@
pkgdatadir: @cfg_pkgdatadir@
docdir: @cfg_docdir@
pdfdir: @cfg_pdfdir@
sysconfdir: @cfg_sysconfdir@

# Compilers and associated flags
# ------------------------------

[compilers]

cc: @CC@
cxx: @CXX@
fc: @FC@
nvcc: @NVCC@
ld: @CS_LD@

cc_version: @cs_cc_version_string@
